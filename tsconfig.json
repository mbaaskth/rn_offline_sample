{
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "jsx": "react",                        // JSX 지원 활성화
      "strict": true,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "baseUrl": ".",
      "paths": {
        "@/*": ["src/*"]
      }
    },
    "exclude": ["node_modules", "babel.config.js", "metro.config.js", "jest.config.js"]
  }